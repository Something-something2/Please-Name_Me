<form class="container mt-5 user-profile">
    <fieldset disabled>

        <div class="container text-center">
            <div class="row mt-3">
                <div class="form-group col text-center">
                    <!-- <label for="inputEmail4">Email:</label> -->
                    <input type="email" class="form-control text-center" id="inputEmail" value="{{user.email}}" placeholder="Email">
                </div>
            </div>
            <div class="row mt-3">
                <div class="form-group col text-center">
                    <!-- <label for="inputPassword4">Password:</label> -->
                    <input type="password" class="form-control text-center" id="inputPassword" value="{{user.password}}"
                        placeholder="Password">
                </div>
            </div>
        </div>


    </fieldset>

    <div class="form-row mt-3">

        <div class="col-sm-12 text-center mt-3">
            <button type="submit" class="btn btn-primary" id="submit-edit-profile">Submit</button>
        </div>

    </div>

</form>

<script src="/javascript/edit-profile.js"></script>